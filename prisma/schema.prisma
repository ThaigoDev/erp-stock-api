generator client {
  provider = "prisma-client-js"
}
enum Status {
  ATIVO
  INATIVO
}


datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  password  String
  email     String   @unique
  createdAt DateTime @default(now())
}

model Category {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  status      Status @default(ATIVO)
  createAt    DateTime @default(now())
}
